<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>dev.maxrn.kanata</string>
    <key>ProgramArguments</key>
    <array>
        <string>/etc/profiles/per-user/maxrn/bin/kanata</string>
        <string>--cfg</string>
        <string>/Users/maxrn/.config/kanata/macbook.kbd</string>
        <string>--cfg</string>
        <string>/Users/maxrn/.config/kanata/mage.kbd</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/kanata/stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/kanata/stderr.log</string>
</dict>
</plist>
